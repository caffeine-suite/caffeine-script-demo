|caffeine-script
import &ArtFoundation

{source, value} = <capture>
  # implicit arrays
  a:
    1 2 3
    4 5 6
    7 8 9

  # :unquotedStrings and more implicit arrays
  match:
    pattern: :blah  action: :boring
    pattern: :zoom  action: :yay!

log
  []
    "" Welcome to CaffeineScript!

    "" Where this code:

    source
    .replace /(^|\n)/g, "$1  "  # indent

    "" generates this:

    "  " + JSON.stringify value

  .join '\n\n'
